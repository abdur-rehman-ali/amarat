      <!-- ======= Contact Single ======= -->
      <section class="contact">
        <div class="container">
          <div class="row">
            <div class="col-sm-12">
              <div class="contact-map box">
                <div id="map" class="contact-map">
                  <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3022.1422937950147!2d-73.98731968482413!3d40.75889497932681!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x89c25855c6480299%3A0x55194ec5a1ae072e!2sTimes+Square!5e0!3m2!1ses-419!2sve!4v1510329142834" width="100%" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                </div>
              </div>
            </div>
            <div class="col-sm-12 section-t8">
              <div class="row">
                <div class="col-md-7">
                    <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
                    <%= render "devise/shared/error_messages", resource: resource %>
                    <div class="row">
                      <div class="col-md-12 mb-3">
                        <div class="form-group">
                        <%= f.email_field :email, autofocus: true, autocomplete: "email",class:"form-control form-control-lg form-control-a" ,placeholder:"Email"%>
                        </div>
                      </div>
                      <div class="col-md-12 mb-3">
                        <div class="form-group">
                        <%= f.text_field :username, class:"form-control form-control-lg form-control-a" ,placeholder:"Username"%>
                        </div>
                      </div>
                       <div class="col-md-12 mb-3">
                        <div class="form-group">
                        <%= f.text_field :phone, class:"form-control form-control-lg form-control-a" ,placeholder:"Phone number"%>
                        </div>
                      </div>
                      <div class="col-md-12 mb-3">
                        <div class="form-group">
                        <%= f.password_field :password, autocomplete: "new-password",class:"form-control form-control-lg form-control-a" ,placeholder:"Password"%>
                        </div>
                      </div>  
                      <div class="col-md-12 mb-3">
                        <div class="form-group">
                        <%= f.password_field :password_confirmation,autocomplete: "new-password",class:"form-control form-control-lg form-control-a" ,placeholder:"Confirm Password"%>
                        </div>
                      </div>
                      <div class="col-md-12 mb-3">
                        <div class="form-group">
                          <%= f.file_field :profile_image%>
                        </div>
                      </div>
                        <div class="col-md-12 text-center">
                        <div class="actions">
                          <%= f.submit "Create account", class:'btn btn-a' %>
                        </div>
                      </div>
                    </div>
                    <%= render "devise/shared/links" %>
                  <% end %>
 
               </div>
                <div class="col-md-5 section-md-t3">
                  <div class="icon-box section-b2">
                    <div class="icon-box-icon">
                      <span class="bi bi-envelope"></span>
                    </div>
                    <div class="icon-box-content table-cell">
                      <div class="icon-box-title">
                        <h4 class="icon-title">Say Hello</h4>
                      </div>
                      <div class="icon-box-content">
                        <p class="mb-1">Email.
                          <span class="color-a">contact@example.com</span>
                        </p>
                        <p class="mb-1">Phone.
                          <span class="color-a">+54 356 945234</span>
                        </p>
                      </div>
                    </div>
                  </div>
                  <div class="icon-box section-b2">
                    <div class="icon-box-icon">
                      <span class="bi bi-geo-alt"></span>
                    </div>
                    <div class="icon-box-content table-cell">
                      <div class="icon-box-title">
                        <h4 class="icon-title">Find us in</h4>
                      </div>
                      <div class="icon-box-content">
                        <p class="mb-1">
                          Manhattan, Nueva York 10036,
                          <br> EE. UU.
                        </p>
                      </div>
                    </div>
                  </div>
                  <div class="icon-box">
                    <div class="icon-box-icon">
                      <span class="bi bi-share"></span>
                    </div>
            
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section><!-- End Contact Single-->
